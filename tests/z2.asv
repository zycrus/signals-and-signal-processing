% % Sampling frequency
f = 225;
fs = 44100;
syms t z;
% % Define the filter coefficients for B(z) and A(z)
[B, A] = butter(4, [50 500]/(fs/2), "bandpass");
[B2, A2] = butter(1, [800 2000]/(fs/2), "bandpass");

% Create Pole-Zero Plot
figure;
zplane(B, A); % Plot poles and zeros
hold on;

% Overlay Unit Circle (frequency path in Z-plane)
theta = linspace(0, 2*pi, 1000); % 1000 points around the circle
x = cos(theta); % Real part of the unit circle
y = sin(theta); % Imaginary part of the unit circle
plot(x, y, 'r--', 'LineWidth', 1.5); % Dashed red line for unit circle

% Enhance Plot
title('Pole-Zero Plot with Unit Circle');
xlabel('Real Part');
ylabel('Imaginary Part');
grid on;
legend('Zeros', 'Poles', 'Unit Circle (\omega path)');

hold off;